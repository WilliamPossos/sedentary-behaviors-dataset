<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.william.data_set">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name=".AndroidCollectAplication"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".includeform.ui.PersonalFormActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".selectactivity.ui.ListActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".measurescollection.ui.StartCollectionActivity"
            android:label="Recolectando"
            android:theme="@style/AppTheme.ActionBar" />
        <activity
            android:name=".validatedata.ui.ValidateActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".completecollection.EndedActivity" />
        <activity android:name=".login.ui.LoginActivity" />
        <activity android:name=".login.ui.RootActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".cleandata.ui.CleanDataActivity">
        </activity>
    </application>

</manifest>